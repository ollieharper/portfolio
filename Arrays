import random
array = [random.randint(0, 100) for i in range(0, 5)]

minimum = 1000
maximum = 0
addition = 0

for counter in array:
    addition += counter
    if counter > maximum:
        maximum = counter
    if counter < minimum:
        minimum = counter

print("The numbers are " + str(array))
print("When added they equal " + str(addition))
print("The lowest number is " + str(minimum))
print("The highest number is " + str(maximum))
